{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "eslint:recommended", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error",
    "max-depth": ["error", 2],
    "import/extensions": "off",
    "no-unused-expressions": "off",
    "no-return-assign": "off",
    "no-alert": "off",
    "consistent-return": "off",
    "no-undef": "off"
  }
}
